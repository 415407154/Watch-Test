<view class="forget"><cu-custom vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText"></view><view slot="content">重置密码</view></cu-custom><view class="content"><view class="main"><view class="tips">忘记了密码？可在此重置新密码。</view><view class="main-list oBorder"><input class="main-input" type="text" maxlength="11" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phoneData','$event',[]]]]]]}}" value="{{phoneData}}" bindinput="__e"/></view><view class="main-list oBorder"><input class="main-input" type="text" maxlength="32" placeholder="请输入新密码" password="{{!showPassword}}" data-event-opts="{{[['input',[['__set_model',['','passData','$event',[]]]]]]}}" value="{{passData}}" bindinput="__e"/><image data-event-opts="{{[['tap',[['isShowPass',['$event']]]]]}}" class="{{['img',showPassword?'cuIcon-attention':'cuIcon-attentionforbid']}}" bindtap="__e"></image></view><view class="main-list oBorder"><input class="main-input" type="number" maxlength="4" placeholder="验证码" data-event-opts="{{[['input',[['__set_model',['','verCode','$event',[]]]]]]}}" value="{{verCode}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getVerCode']]]]}}" class="{{['vercode',[(second>0)?'vercode-run':'']]}}" bindtap="__e">{{getVerCodeSecond}}</view></view></view><button data-event-opts="{{[['tap',[['startRePass']]]]}}" class="{{['buttonBorder',!isRotate?'dlbutton':'dlbutton_loading']}}" bindtap="__e"><view class="{{[isRotate?'rotate_loop':'']}}"><block wx:if="{{isRotate}}"><text class="cuIcon-loading1 "></text></block><block wx:if="{{!isRotate}}"><text>重置密码</text></block></view></button></view></view>